<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script>
  function DisplayError(string, callback) {
    swal("Error!", string, "error").then(() => {
      callback();
    });
  }

  function onClickEnroll() {
    const onUsing = "{{auth}}";
    if (onUsing === "true") {
      const newData = {
        courseId: "{{course.CourseID}}",
      };
      $.post("/student/enroll", newData, function (data, status) {
        console.log(data);
        if (data === true) {
          swal(
            "Success!",
            "Your have enrolled successfully!",
            "success"
          ).then((value) => {
            if (value === true) {
              location.reload();
            }
          });
        } else {
          DisplayError(data);
        }
      });
    } else {
      location.replace("/account/login");
    }
    console.log("onClickEnroll");
  }

  function onAddtoWatchList() {
    const onUsing = "{{auth}}";
    if (onUsing === "true") {
      const newData = {
        courseId: "{{course.CourseID}}",
      };
      $.post("/student/watchlist", newData, function (data, status) {
        console.log(data);
        if (data === true) {
          swal("Success!", "Added!", "success").then((value) => {
            if (value === true) {
              location.reload();
            }
          });
        } else {
          DisplayError(data);
        }
      });
    } else {
      location.replace("/account/login");
    }
    console.log("onAddtoWatchList");
  }

  function onClickPostCommit() {
    var count = 0;
    if (jQuery("#feedback--stars-item-1")[0].checked === true) count = 1;
    if (jQuery("#feedback--stars-item-2")[0].checked === true) count = 2;
    if (jQuery("#feedback--stars-item-3")[0].checked === true) count = 3;
    if (jQuery("#feedback--stars-item-4")[0].checked === true) count = 4;
    if (jQuery("#feedback--stars-item-5")[0].checked === true) count = 5;

    const comment = jQuery("#comment").val();

    const onUsing = "{{auth}}";
    if (onUsing === "true") {
      const newData = {
        studentId: "{{authUser.id}}",
        courseId: "{{course.CourseID}}",
        star: count,
        comment: comment,
      };
      $.post("/student/rate/detail", newData, function (data, status) {
        console.log(data);
        if (data === true) {
          swal("Success!", "Added!", "success").then((value) => {
            if (value === true) {
              location.reload();
            }
          });
        } else {
          DisplayError(data);
        }
      });
    } else {
      DisplayError(
        "You have to login before posting a comment or rating",
        () => {
          location.replace("/account/login");
        }
      );
    }
  }

  function onClickFinish(element) {
    const onUsing = "{{auth}}";
    console.log(element);
    if (onUsing === "true") {
      const newData = {
        studentId: "{{authUser.id}}",
        courseId: "{{course.CourseID}}",
        sectionId: jQuery(element).data("sectionid"),
      };
      $.post("/student/finish", newData, function (data, status) {
        console.log(data);
        if (data === true) {
          swal(
            "Thank you!",
            "Thank you for finished this section!",
            "success"
          ).then((value) => {
            if (value === true) {
              location.reload();
            }
          });
        } else {
          DisplayError(data);
        }
      });
    } else {
      location.replace("/account/login");
    }
  }
</script>

