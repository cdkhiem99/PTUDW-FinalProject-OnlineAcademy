<script>
const scrollHeight_ = Math.max(
  document.body.scrollHeight, document.documentElement.scrollHeight,
  document.body.offsetHeight, document.documentElement.offsetHeight,
  document.body.clientHeight, document.documentElement.clientHeight
);

const innerHeight_ = window.innerHeight;

const enrollStatus = document.querySelector(".udlite-sr-only.group4-check-isEnrolled").innerText;
const inwatchlistStatus = document.querySelector(".udlite-sr-only.group4-check-isInWatchList").innerText;
console.log("isEnrolled =", enrollStatus);
console.log("isInWatchList =", inwatchlistStatus);
let factorScrollToBottom = 1.4;
if (enrollStatus !== inwatchlistStatus) {
  factorScrollToBottom = 1.5;
}
else if (enrollStatus === "false" &&
    inwatchlistStatus === "false") {
  factorScrollToBottom = 1.6;
}

window.addEventListener("scroll", function(){
  const scrollTop_ = innerHeight_ * 1.6;
  const sidebar = document.querySelector(".course-landing-page_sidebar-container");
  if (scrollTop_ < window.pageYOffset &&
      window.pageYOffset < (scrollHeight_ - innerHeight_ * factorScrollToBottom))
  {
    sidebar.classList.add("sidebar-container--fixed--2xu7a");
    sidebar.classList.remove("sidebar-container--inflow-bottom--2vTz_");
  }
  else if (window.pageYOffset < scrollTop_) {
    sidebar.classList.remove("sidebar-container--fixed--2xu7a");
    sidebar.classList.remove("sidebar-container--inflow-bottom--2vTz_");
  }
  else {
    sidebar.classList.remove("sidebar-container--fixed--2xu7a");
    sidebar.classList.add("sidebar-container--inflow-bottom--2vTz_");
  }
  {{!-- console.log(window.pageYOffset,
              scrollHeight_ - innerHeight_ * factorScrollToBottom,
              factorScrollToBottom
            ); --}}
});

document.querySelector(".course-landing-page__main-content.component-margin").style.marginTop = 0;

const firstSection = document.querySelector(".section--panel--1tqxC.panel--panel--3NYBX");
if (firstSection) {
  firstSection.classList.add("section--first-panel--2fbBU");
}

const starWrappers = document.querySelectorAll(".star-rating--star-wrapper--2eczq.star-rating--large--3T9Yf");
for (let i = 0; i < starWrappers.length; ++i) {
  const rating = starWrappers[i].querySelector("span.udlite-sr-only").innerText;
  const stars = parseInt(rating.match(/(\d+)/));

  let j = 0;
  while (j++ < stars) {
    starWrappers[i].innerHTML +=
      `<i class="fas fa-star" style="color: #eb8a2f;"></i>`;
  }
  do {
    starWrappers[i].innerHTML +=
      `<i class="far fa-star" style="color: #eb8a2f;"></i>`;
  } while (j++ < 5);
}
</script>
