{{#with listByFields}}
<div class="card">
  <h4 class="card-header">
    {{CourseName}}
  </h4>
  <div class="card-body">
    <img src="/imgs/course/{{CourseID}}/photo.png" alt="{{this.CourseName}}">
    <p class="card-text mt-3">Price: <span class="text-danger font-weight-bold">{{Price}}</span></p>
    <p class="card-text">
      {{{this.fullDescription}}}
    </p>
  </div>
  <div class="card-footer text-muted">
    <small>Last upadated 3 minutes ago.</small>
  </div>
</div>
{{/with}}
